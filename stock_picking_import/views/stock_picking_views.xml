<odoo>
  <data>
    <record id="stock_picking_import_view_form" model="ir.ui.view">
      <field name="name">Stock Picking Import View</field>
      <field name="model">stock_picking.import</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Import Product From CSV File To Stock Move">
          <group>
            <field name="data_file" nolabel="1" filename="file_name" />
            <button name="import_file" string="Import" type="object" />
            <field name="file_name" invisible="1" />
          </group>
          <footer>
            <button string="Cancel" class="btn-default" special="cancel"/>
          </footer>
        </form>
      </field>
    </record>
    <act_window name="Import Product"
                res_model="stock_picking.import"
                src_model="stock.picking"
                target="new"
                view_mode="form"
                view_id="stock_picking_import_view_form"
                id="stock_picking_import_action"/>
  </data>
</odoo>