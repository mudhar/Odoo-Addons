<odoo>
    <data>
        <record id="production_journal_wip_wizard_view_form" model="ir.ui.view">
            <field name="name">production_journal.wip_wizard.view.form</field>
            <field name="model">production_journal.wip_wizard</field>
            <field name="arch" type="xml">
                <form string="Adjust WIP Material Differ">
                    <field name="production_id" invisible="1"/>
                    <group>
                        <group>
                            <field name="account_expense_material_id"/>
                            <field name="amount_wip_differ"/>
                        </group>
                        <group>
                            <field name="amount_total"/>
                        </group>

                    </group>
                    <footer>
                        <button name="action_confirm" type="object" string="Confirm"
                                class="btn-primary" colspan="1"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>