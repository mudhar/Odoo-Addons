<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Inherit Form View to Modify it -->
        <record id="view_company_form_inherit_assembly_account" model="ir.ui.view">
            <field name="name">view.company.form.inherit.assembly.account</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='location_reject_id']" position="after">
                    <!-- Add your fields or attributes here -->
                    <field name="account_expense_material_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>