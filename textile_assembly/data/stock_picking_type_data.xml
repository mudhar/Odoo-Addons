        </record>

        <record forcecreate="True" id="picking_cmt_consume" model="stock.picking.type">
            <field name="name">CMT Consume</field>
            <field name="sequence_id" ref="po_picking_multi_sequence.seq_stock_picking_materials_ougoing"/>
            <field name="code">internal</field>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="show_reserved">True</field>
            <field name="use_create_lots">False</field>
            <field name="use_existing_lots">True</field>
            <field name="default_location_src_id" ref="stock.stock_location_stock"/>
            <field name="default_location_dest_id" ref="stock.location_production"/>
        </record>

    </data>
</odoo>