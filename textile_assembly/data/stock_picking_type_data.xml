<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record forcecreate="True" id="picking_cmt_produce" model="stock.picking.type">
            <field name="name">CMT Produce</field>
            <field name="sequence_id" ref="po_picking_multi_sequence.seq_stock_picking_goods_incoming"/>
            <field name="code">internal</field>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="show_reserved">True</field>
            <field name="use_create_lots">False</field>
