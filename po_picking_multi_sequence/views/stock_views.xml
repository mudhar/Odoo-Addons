<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Inherit Form View to Modify it -->
        <record id="view_warehouse_inherit_multi_sequence" model="ir.ui.view">
            <field name="name">view.warehouse.inherit.multi.sequence</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='code']" position="after">
                    <!-- Add your fields or attributes here -->
                    <field name="is_warehouse"  attrs="{'invisible': [('is_store','=',True)]}"/>
                    <field name="is_store" attrs="{'invisible': [('is_warehouse','=',True)]}"/>
                </xpath>

            </field>
        </record>
        <!-- Inherit Form View to Modify it -->
        <record id="view_picking_form_inherit_multi_sequence" model="ir.ui.view">
            <field name="name">Select Multi Sequence</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='group_id']" position="after">
                    <!-- Add your fields or attributes here -->
                    <field name="product_select_type" readonly="1" groups="base.group_no_one"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>