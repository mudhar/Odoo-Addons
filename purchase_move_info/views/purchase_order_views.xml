<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="purchase_order_tree_inherit_info_view" model="ir.ui.view">
        <field name="name">purchase.order.tree.inherit.info.view</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_tree"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='state']" position="after">
                <!-- Add new fields here -->
                <field name="total_qty_received" sum="Total Received"/>
            </xpath>

        </field>
    </record>
  </data>
</odoo>